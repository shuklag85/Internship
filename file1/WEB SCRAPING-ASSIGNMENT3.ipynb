    "import time\n",
    "import pandas as pd\n",
    "from selenium import webdriver\n",
    "from selenium.common.exceptions import NoSuchElementException\n",
    "from selenium.webdriver.common.by import By\n",
    "\n",
    "\n",
    "\n",
    "# URLs to scrape\n",
    "URL_list = []\n",
    "\n",
    "start = 1\n",
    "end = 3\n",
    "\n",
    "for page in range(start, end):\n",
    "    # Find all product URLs on the current page\n",
    "    urls = driver.find_elements(By.XPATH, '//a[@class=\"a-link-normal s-underline-text s-underline-link-text s-link-style a-text-normal\"]')\n",
    "    \n",
    "    for url in urls:\n",
    "        URL_list.append(url.get_attribute(\"href\"))\n",
    "    \n",
    "    # Navigate to the next page\n",
    "    next_button = driver.find_element(By.XPATH, '//a[@class=\"s-pagination-item s-pagination-next s-pagination-button s-pagination-separator\"]')\n",
    "    next_button.click()\n",
    "    time.sleep(3)\n",
    "\n",
    "# Function to get element value or return '-' if element not found\n",
    "def get_element_val(driver, paramXp):\n",
    "    try:\n",
    "        retval =driver.find_element(By.XPATH, paramXp).text\n",
    "    except NoSuchElementException:\n",
    "        retval = \"-\"\n",
    "    return retval\n",
    "\n",
    "# Lists to store scraped data\n",
    "Brand_Name = []\n",
    "Prod_Name = []\n",
    "Price = []\n",
    "Return = []\n",
    "Delivery = []\n",
    "Availability = []\n",
    "\n",
    "# Iterate over each product page URL\n",
    "for product_page in URL_list:\n",
    "    driver.get(product_page)\n",
    "    time.sleep(3)\n",
    "    \n",
    "    # Get product details\n",
    "    BR_name = get_element_val(driver, '//tr[@class=\"a-spacing-small po-brand\"]/td[2]/span')\n",
    "    prd = get_element_val(driver, '//span[@class=\"a-size-large product-title-word-break\"]')\n",
    "    pr = get_element_val(driver, '//div[@class=\"a-section a-spacing-none aok-align-center aok-relative\"]/span/span/span[2]')\n",
    "    Rtn = get_element_val(driver, '//div[@id=\"RETURNS_POLICY\"]/span/div[2]/span')\n",
    "    Del = get_element_val(driver, '//div[@id=\"mir-layout-DELIVERY_BLOCK-slot-PRIMARY_DELIVERY_MESSAGE_LARGE\"]/span/span[1]')\n",
    "    Avl = get_element_val(driver, '//div[@id=\"availability\"]/span')\n",
    "    \n",
    "    \n",
    "    # Append data to lists\n",
    "    Brand_Name.append(BR_name)\n",
    "    Prod_Name.append(prd)\n",
    "    Price.append(pr)\n",
    "    Return.append(Rtn)\n",
    "    Delivery.append(Del)\n",
    "    Availability.append(Avl)\n",
    "    \n",
    "    break # system is hanging hence collecting only 1 record\n",
    "\n",
    "\n",
    "\n",
    "# Create DataFrame from scraped data\n",
    "df = pd.DataFrame({\n",
    "    'Brand_Name': Brand_Name,\n",
    "\n",
    "    'Prod_Name': Prod_Name,\n",
    "    'Price': Price,\n",
    "    'Return': Return,\n",
    "    'Delivery': Delivery,\n",
    "    'Availability': Availability,\n",
    "    'URL_list':URL_list\n",
    "})\n",
    "\n",
    "# Display DataFrame\n",
    "df\n",
    "\n",
    "\n"
   ]
