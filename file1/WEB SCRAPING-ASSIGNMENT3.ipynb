    "from selenium import webdriver\n",
    "from selenium.webdriver.common.by import By\n",
    "import time\n",
    "import requests\n",
    "\n",
    "# Initialize WebDriver\n",
    "driver = webdriver.Chrome()\n",
    "driver.get('https://www.bing.com/images/feed?form=Z9LH')\n",
    "driver.maximize_window()\n",
    "time.sleep(3)\n",
    "\n",
    "URL_list=[]\n",
    "\n",
    "\n",
    "val = input(\"Type of product: \")\n",
    "search_input = driver.find_element(By.XPATH,'//input[@id=\"sb_form_q\"]')\n",
    "search_input.send_keys(val)\n",
    "search_input.submit()\n",
    "\n",
    "time.sleep(3)\n",
    "        \n",
    "#//img[@class=\"YQ4gaf\"]\n",
    "#//g-img[@class=\"mNsIhb\"]/img\n",
    "\n",
    "urls = driver.find_elements(By.XPATH,'//div[@class=\"mimg\"]/img')\n",
    "\n",
    "\n",
    "for url in urls:\n",
    "    source=url.get_attribute(\"src\")\n",
    "    if source is not None:\n",
    "        if (source[0:4]=='http'):\n",
    "            URL_list.append(source)\n",
    "#print(len(URL_list))\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "for i in range(len(URL_list)):\n",
    "    if i>10:\n",
    "        break\n",
    "    print(\"Downloading {0} of {1} urls\".format(i,10))\n",
    "    response= requests.get(URL_list[i])\n",
    "    time.sleep(3)\n",
    "    file=open(r'E:\\D\\Data Trained\\project'+str(i)+'.jpg','wb')\n",
    "    file.write(response.content)\n",
    "    file.close()\n",
    "    \n",
    "df=pd.DataFrame({\n",
    "    'URL_list':URL_list\n",
    "})\n",
    "\n",
    "df\n",
    "    "
