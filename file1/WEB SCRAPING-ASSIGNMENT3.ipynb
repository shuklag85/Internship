    "from selenium import webdriver\n",
    "from selenium.webdriver.common.by import By\n",
    "import time\n",
    "import requests\n",
    "\n",
    "# Initialize WebDriver\n",
    "driver = webdriver.Chrome()\n",
    "driver.get('https://www.flipkart.com/')\n",
    "driver.maximize_window()\n",
    "time.sleep(3)\n",
    "\n",
    "Brand_Name=[]\n",
    "Smartphone_name=[]\n",
    "Colour=[]\n",
    "RAM=[]\n",
    "Storage=[]\n",
    "Primary_Camera=[] \n",
    "Secondary_Camera=[]\n",
    "Display_Size=[]\n",
    "Battery_Capacity=[] \n",
    "Price=[]\n",
    "URL_list=[]\n",
    "\n",
    "search_input = driver.find_element(By.XPATH,'//input[@class=\"Pke_EE\" and @title=\"Search for Products, Brands and More\"]')\n",
    "search_input.send_keys('Oneplus Nord')\n",
    "search_input.submit()\n",
    "time.sleep(3)\n",
    "urls = driver.find_elements(By.XPATH, '//a[@class=\"CGtC98\"]')\n",
    "    \n",
    "for url in urls:\n",
    "    URL_list.append(url.get_attribute(\"href\"))\n",
    "\n",
    "# Function to get element value or return '-' if element not found\n",
    "def get_element_val(driver, paramXp):\n",
    "    try:\n",
    "        retval =driver.find_element(By.XPATH, paramXp).text\n",
    "    except NoSuchElementException:\n",
    "        retval = \"-\"\n",
    "    return retval\n",
    "\n",
    "a=1\n",
    "# Iterate over each product page URL\n",
    "for product_page in URL_list:\n",
    "    driver.get(product_page)\n",
    "    time.sleep(3)\n",
    "    button=driver.find_element(By.XPATH,'//button[@class="QqFHMw _4FgsLt"]')\n",
    "    button.click()\n",
    "    time.sleep(1)\n",
    "    \n",
    "    \n",
    "    # Append data to lists\n",
    "    Brand_Name.append('Oneplus Nord')\n",
    "    Smartphone_name.append(get_element_val(driver, '//div[@class=\"GNDEQ-\"][1]/table[1]/tbody/tr[3]/td[2]'))\n",
    "    Colour.append(get_element_val(driver, '//div[@class=\"GNDEQ-\"][1]/table[1]/tbody/tr[4]/td[2]'))\n",
    "    RAM.append(get_element_val(driver, '//div[@class=\"GNDEQ-\"][4]/table/tbody/tr[2]/td[2]'))\n",
    "    Storage.append(get_element_val(driver, '//div[@class=\"GNDEQ-\"][4]/table/tbody/tr[1]/td[2]'))\n",
    "    Primary_Camera.append(get_element_val(driver, '//div[@class=\"GNDEQ-\"][5]/table/tbody/tr[1]/td[2]')) \n",
    "    Secondary_Camera.append(get_element_val(driver, '//div[@class=\"GNDEQ-\"][5]/table/tbody/tr[2]/td[2]'))\n",
    "    Display_Size.append(get_element_val(driver, '//div[@class=\"GNDEQ-\"][2]/table/tbody/tr[1]/td[2]'))\n",
    "    Battery_Capacity.append(get_element_val(driver, '//div[@class=\"GNDEQ-\"][7]/table/tbody/tr[1]/td[2]')) \n",
    "    Price.append(get_element_val(driver, '//div[@class=\"Nx9bqj CxhGGd\"]'))\n",
    "    #URL_list.append(get_element_val(driver, 'dddd'))\n",
    "    \n",
    "    break # system is hanging hence collecting only 1 record\n",
    "    \n",
    "\n",
    "\n",
    "# Create DataFrame from scraped data\n",
    "df = pd.DataFrame({\n",
    "    'Brand_Name':Brand_Name,\n",
    "    'Smartphone_name':Smartphone_name,\n",
    "    'Colour':Colour,\n",
    "    'RAM':RAM,\n",
    "    'Storage':Storage,\n",
    "    'Primary_Camera': Primary_Camera,\n",
    "    'Secondary_Camera':Secondary_Camera,\n",
    "    'Display_Size':Display_Size,\n",
    "    'Battery_Capacity': Battery_Capacity,\n",
    "    'Price':Price\n",
    "    #'URL_list':URL_list\n",
    "})\n",
    "\n",
    "# Display DataFrame\n",
    "df\n",
    "\n",
    "    \n",
    "\n"
